{
  "name": "qiankun-demo",
  "version": "1.0.0",
  "description": "qiankun微框架demo",
  "main": "index.js",
  "author": "zhangyuanliang",
  "license": "MIT",
  "scripts": {
    "install": "npm-run-all --serial install:*",
    "install:main": "cd main && yarn",
    "install:app1": "cd app1 && yarn",
    "install:app2": "cd app2 && yarn",
    "start": "npm-run-all --parallel start:*",
    "start:main": "cd main && yarn start",
    "start:app1": "cd app1 && yarn start",
    "start:app2": "cd app2 && yarn start",
    "build": "npm-run-all --parallel build:*",
    "build:main": "cd main && yarn build",
    "build:app1": "cd app1 && yarn build"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
